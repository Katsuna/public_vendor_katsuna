<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- Set default input method to Katsuna Keyboard -->
    <string name="def_input_method">com.katsuna.keyboard/.ui.SoftKeyboard</string>
    <string name="def_enable_input_methods" translatable="false">com.katsuna.keyboard/.ui.SoftKeyboard</string>

    <!-- Set default Backup service to Google -->
    <bool name="def_backup_enabled">false</bool>
    <string name="def_backup_transport">com.google.android.gms/.backup.BackupTransportService</string>

    <!-- Non Market Apps -->
    <bool name="def_install_non_market_apps">true</bool>
    
    <!-- Animation scales (Development Settings) -->
    <fraction name="def_window_animation_scale">50%</fraction>
    <fraction name="def_window_transition_scale">50%</fraction>

    <!-- Default Screen Timeout (needs another change in Settings repo, FALLBACK_SCREEN_TIMEOUT_VALUE -->
    <integer name="def_screen_off_timeout">120000</integer>

</resources>
